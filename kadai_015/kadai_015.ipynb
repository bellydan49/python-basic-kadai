        "class Human:\n",
        "  def __init__(self, name, age):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "  def print_info(self):\n",
        "        print(f\"{self.name}は{self.age}歳です\")\n",
        "\n",
        "taro = Human(\"太郎\", 15)\n",
        "hanako = Human(\"花子\", 18)\n",
        "\n",
        "taro.print_info()\n",
        "hanako.print_info()\n"
      ]
    }
  ]
