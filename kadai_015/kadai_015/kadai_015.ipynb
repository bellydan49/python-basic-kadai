        "class Human:\n",
        "    def __init__(self, name, age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "\n",
        "    def printinfo(self):\n",
        "        print(self.name)\n",
        "        print(self.age)\n",
        "\n",
        "\n",
        "taro = Human(\"太郎\", 15)\n",
        "hanako = Human(\"花子\", 18)\n",
        "\n",
        "taro.printinfo()\n",
        "hanako.printinfo()\n"
      ]
